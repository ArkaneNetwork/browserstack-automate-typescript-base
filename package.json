    "browserstack-local": "^1.4.5",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "mocha": "^7.0.1",
    "dotenv": "^8.2.0",
    "tslib": "^1.10.0",
    "mocha-typescript": "^1.1.17",
    "selenium-webdriver": "^4.0.0-alpha.5"
  },
  "devDependencies": {
    "@types/chai": "^4.2.8",
    "@types/chai-as-promised": "^7.1.2",
