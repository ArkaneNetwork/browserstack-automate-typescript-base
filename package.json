    "type": "git",
    "url": "git+https://github.com/ArkaneNetwork/browserstack-automate-typescript-base"
  },
  "bugs": {
    "url": "https://github.com/ArkaneNetwork/browserstack-automate-typescript-base/issues"
  },
  "dependencies": {
